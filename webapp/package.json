{
  "name": "webapp",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "build:prod": "NODE_ENV=production ng build --prod",
    "prettier:check": "prettier --check \"src/**/*.ts\" \"e2e/**/*.ts\"",
    "prettier:format": "prettier --write \"src/**/*.ts\" \"e2e/**/*.ts\"",
    "test": "ng test",
    "lint": "ng lint && npm run prettier:check",
    "e2e": "ng e2e",
    "generate-icons": "svg-generator && eslint --fix \"src/app/svg/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.1",
    "@angular/common": "~12.2.1",
    "@angular/compiler": "~12.2.1",
    "@angular/core": "~12.2.1",
    "@angular/forms": "~12.2.1",
    "@angular/platform-browser": "~12.2.1",
    "@angular/platform-browser-dynamic": "~12.2.1",
    "@angular/router": "~12.2.1",
    "@ngneat/svg-icon": "^3.2.0",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/entity": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "rxjs": "~6.6.0",
    "tailwindcss": "^2.2.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.1",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/schematics": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "~12.2.1",
    "@angular/compiler-cli": "~12.2.1",
    "@ngneat/svg-generator": "^2.0.0",
    "@types/jasmine": "^3.8.2",
    "@types/node": "^16.6.1",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "autoprefixer": "^10.3.1",
    "eslint": "^7.32.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "ts-node": "~10.2.0",
    "typescript": "~4.3.5"
  },
  "svgGenerator": {
    "outputPath": "./src/app/svg",
    "prefix": "app",
    "srcPath": "./src/assets/svg",
    "svgoConfig": {
      "plugins": [
        {
          "removeDimensions": true,
          "cleanupAttrs": true
        }
      ]
    }
  }
}
